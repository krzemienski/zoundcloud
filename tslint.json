{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "arrow-return-shorthand": [true, "multiline"],
    "indent": [true, "spaces"],
    "max-line-length": [true, 120],
    "quotemark": [true, "single"],
    "ter-indent": [true, 2]
  },
  "rulesDirectory": []
}