{
  "name": "ZoundCloud",
  "description": "Adds download buttons to SoundCloud pages.",
  "version": "2.0.0",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background.js"
    ],
    "persistent": false
  }
}