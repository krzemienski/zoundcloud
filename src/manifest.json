{
  "name": "ZoundCloud",
  "description": "Adds download buttons to SoundCloud pages.",
  "version": "2.0.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation",
    "*://soundcloud.com/*",
    "*://api.soundcloud.com/*"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "vendor.js",
      "eventPage.js"
    ],
    "persistent": false
  }
}