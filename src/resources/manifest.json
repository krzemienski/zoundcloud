{
  "name": "ZoundCloud",
  "description": "Adds download buttons to SoundCloud pages.",
  "version": "1.1.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "downloads",
    "tabs",
    "webNavigation",
    "*://soundcloud.com/*",
    "*://api.soundcloud.com/*"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background.js"
    ],
    "persistent": false
  }
}